{
  "name": "mostux-todomvc",
  "version": "0.0.1",
  "description": "",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "extensions": [
            ".es6",
            ".jsx",
            ".js"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify src/app.jsx --extension=.jsx --extension=.es6 | java -jar bin/compiler.jar > public/app.js",
    "start": "ecstatic -p 8000 public",
    "watch": "watchify -d src/app.jsx --extension=.jsx --extension=.es6 -o public/app.js -dv",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  },
  "dependencies": {
    "ramda": "^0.22.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-most": "^0.6.5",
    "union-type": "^0.3.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-jest": "^16.0.0",
    "babel-plugin-ramda": "^1.1.6",
    "babel-plugin-transform-react-jsx": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "ecstatic": "^1.3.1",
    "envify": "^3.4.1",
    "jest-cli": "^0.7.0",
    "react-addons-test-utils": "^15.3.2",
    "watchify": "^3.6.1"
  },
  "author": "Jichao Ouyang",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "ramda"
    ]
  }
}
