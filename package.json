{
  "name": "xreact",
  "version": "0.0.0",
  "description": "Functional Reactive State Container for React",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcouyang/react-most.git"
  },
  "main": "lib/x.js",
  "typings": "types/x.d.ts",
  "files": [
    "src",
    "lib",
    "types"
  ],
  "scripts": {
    "license": "(cat LICENSE.txt; cat react-most.js) > react-most.licensed.js && mv react-most.licensed.js react-most.js",
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run build",
    "testWDebugger": "node --harmony $(which bugger) ./node_modules/jest-cli/bin/jest.js --runInBand"
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "@reactivex/rxjs": "^5.4.0",
    "most": "^1.2.2",
    "most-subject": "^5.3.0",
    "react": "^15.5.4"
  },
  "devDependencies": {
    "@reactivex/rxjs": "^5.4.0",
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.18",
    "@types/react": "^15.0.22",
    "create-react-class": "^15.5.3",
    "enzyme": "^2.8.2",
    "jest": "^20.0.0",
    "lodash": "^4.0.0",
    "most": "^1.2.2",
    "most-subject": "^5.3.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.0.4",
    "ts-jest": "^20.0.2",
    "typescript": "^2.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(tsx|ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "roots": [
      "src"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "author": "Jichao Ouyang",
  "license": "MIT",
  "keywords": [
    "Rx",
    "RxJS",
    "ReactiveX",
    "ReactiveExtensions",
    "Streams",
    "Observables",
    "Observable",
    "Stream",
    "React",
    "most",
    "mostjs",
    "FRP",
    "Reactive",
    "ES6",
    "ES2015",
    "xreact",
    "typescript"
  ]

}
